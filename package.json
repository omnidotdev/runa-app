    "@parcel/watcher": "^2.5.1",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query-devtools": "^5.76.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "dotenv": "^16.5.0",
