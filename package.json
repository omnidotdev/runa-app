    "@tiptap/extension-placeholder": "^2.12.0",
    "@tiptap/extension-task-item": "^2.12.0",
    "@tiptap/extension-task-list": "^2.12.0",
    "@tiptap/react": "^2.12.0",
    "@tiptap/starter-kit": "^2.12.0",
    "@tiptap/suggestion": "^2.12.0",
    "chrono-node": "^2.8.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.11.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.6",
